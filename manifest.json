{
  "manifest_version": 2,

  "name": "nKemono",
  "description": "Inspired by nCage.  It replaces all of your images by Kemono Friends",
  "version": "4.0",

  "background":{
    "scripts": ["background.js"],
    "persistent": false
  },

  "browser_action": {
    "default_icon": "icon.png"
  },

  "permissions": [
    "storage"
  ],

  "content_scripts": [{
    "matches": [
      "http://*/*",
      "https://*/*"
    ],
    "js": [
      "kemono.js"
    ],
    "run_at": "document_end"
  }]
}